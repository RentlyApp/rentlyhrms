{
    "version": 2,
    "builds": [
        {
            "src": "horilla/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "horilla/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "horilla.settings"
    }
} 